{
  "name": "CORS Detector",
  "description": "Detect the CORS Vul Url",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "debugger",
    "tabs",
    "background",
    "pageCapture",
    "unlimitedStorage",
    "notifications",
    "declarativeContent",
    "<all_urls>"
  ],
  "homepage_url": "https://github.com/lhtest429",
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "CORS Detector"
  },
  "manifest_version": 2

}